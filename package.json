		"@types/three": ">=0.139.0",
		"at-at": "^1.1.0",
		"docsify-cli": "trusktr/docsify-cli#add-cors",
		"five-server": "^0.3.1",
		"husky": "^8.0.3",
		"jsdoctypeparser": "^5.0.1",
		"lerna": "4.0.0",
		"lint-staged": "^14.0.1",
		"mkdirp": "^0.5.1",
		"prettier": "3.0.3",
		"readem": "^0.2.7",
		"rimraf": "^3.0.0",
		"surge": "^0.23.0",
		"three": ">=0.139.0",
		"typescript": "4.6.3"
	},
	"repository": {
